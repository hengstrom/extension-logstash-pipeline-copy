{
    "manifest_version": 2,
    "name": "Logstash pipeline conf saver",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "description": "Send managed Logstash pipelines to git",
    "permissions": [
      "activeTab",
      "downloads"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  